<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <link href="https://fonts.googleapis.com/css?family=Baloo+Bhaina" rel="stylesheet">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {% stylesheets 'http://localhost/nendempas/web/bundles/nnpplatform/css/main.css' %}
          <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
    </head>
    <body>
        {% block entete %}
        <div class="container-fluid">
            <section class="top-header">
                <div class="row">
                    <div class="col-md-2"></div>
                    <div class="col-md-4"></div>
                    <div class="col-md-4 reseaux">
                        <div class="icon"><img src="{{ asset('bundles/nnpplatform/images/facebook.png')}}" /></div>
                        <div class="icon"><img src="{{ asset('bundles/nnpplatform/images/twitter.png')}}" /></div>
                    </div>
                    <div class="col-md-2"></div>
                </div>
            </section>
        </div>
        <div class="container">
            <header>
                <div class="row">
                    <div class="col-md-4"><img src="{{ asset('bundles/nnpplatform/images/logo.png')}}" width="300px" /></div>
                    <div class="col-md-8"></div>
                </div>
            </header>
        </div>
        {% endblock %}

        {% block body %}
        {% endblock %}
        
        {% block javascripts %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        {% endblock %}
    </body>
</html>
